	"types": "build-types",
	"dependencies": {
		"@wordpress/api-fetch": "file:../api-fetch",
		"@wordpress/data": "file:../data",
		"@wordpress/deprecated": "file:../deprecated"
	},
	"peerDependencies": {
		"react": "^18.0.0"
	},
	"publishConfig": {
		"access": "public"
